[
  {
    "description": "这个一个会返回cookies信息的get请求",
    "request": {
      "uri": "/getcookies",
      "method": "get"
    },
    "response": {
      "cookies": {
        "login": "true"
      },
      "text": "获得cookies信息成功"
    }

  },
  {
    "description": "带cookie的get请求",
    "request": {
      "uri": "/get/with/cookie",
      "method": "get",
      "cookies": {
        "login": "true"
      }
    },
    "response": {
      "text": "这是一个带cookie才能访问的get请求"
    }
  },
  {
    "description": "post请求",
    "request": {
      "uri": "/postwithpost",
      "method": "post",
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "huhansan",
        "age": "18"
      }
    },
    "response": {
      "status": 200,
      "json": {
        "huhansan": "success",
        "status": 1
      }
    }
  }
]