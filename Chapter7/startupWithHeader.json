[
  {
    "description": "这是一个带header信息的post请求",
    "request": {
      "uri": "/postwithheaders",
      "method": "post",
      "headers": {
        "content-type": "application/json"
      },
      "json": {
        "name": "hedy",
        "sex": "media"
      }
    },
    "response": {
      "status": 200,
      "json": {
        "hedy": "success",
        "status": 1
      }
    }
  }
]